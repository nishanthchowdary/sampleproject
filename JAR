pipeline{
 agent any
   stages{
    stage{
      steps{
      // Get the some coad from the git clone
         git 'https://github.com/nishanthchowdary/sampleproject.git'
      }
    }
    stage('build'){
      steps{
        tools {
    maven 'Maven'
     // build the war file
        }
       sh 'mvn clean install package'
      }
     }  
   }
}
